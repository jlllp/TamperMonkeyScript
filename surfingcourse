// ==UserScript==
// @name         继续教育刷网课
// @namespace    http://tampermonkey.net/
// @version      1.0.4
// @description  网课视频自动播放下一集 有问题可以通过邮箱cheat_lipeng@foxmail.com反馈问题
// @author       JllLp
// @match      *://cqlg.360xkw.com/gxplatform/gxlearningcenter/learning.html*
// @match      *://cqlg.360xkw.com/gxplatform/gxlearningcenter/rebackVideo.html*
// @icon         data:image/jpeg;base64,
// @license      MIT
// @grant        none
// @downloadURL https://update.greasyfork.org/scripts/436145/%E7%BB%A7%E7%BB%AD%E6%95%99%E8%82%B2%E5%88%B7%E7%BD%91%E8%AF%BE.user.js
// @updateURL https://update.greasyfork.org/scripts/436145/%E7%BB%A7%E7%BB%AD%E6%95%99%E8%82%B2%E5%88%B7%E7%BD%91%E8%AF%BE.meta.js
// ==/UserScript==

(function () {
    'use strict';

    // 获取视频播放元素
    window.onload = function () {
        console.log("自动播放 -- 脚本加载成功");
        const video = document.getElementById("live_video");
        // 判断video不为空
        if (video === null) {
            console.log("自动播放 -- 视频播放元素为空");
            return;
        }
        // 监听播放完成时间
        video.addEventListener('ended', delayPlay);
        /**
         * 延迟播放下一个视频
         */
        function delayPlay() {
            // 生成3秒到6秒之间的随机延迟
            const delay = Math.random() * 3000 + 3000;
            // 获取播放列表元素，并将它转换为数组
            const list = Array.from(document.getElementsByClassName("layui-colla-content layui-show")[0].children[0].children);
            // 获取当前视频索引
            const currentVideoIndex = getCurrentVideoIndex(list);
            console.log(`自动播放 -- 视频播放进度：[${currentVideoIndex + 1}] - [${list.length}]`);
            setTimeout(() => {
                // 准备播放下一视频
                const nextVideo = list[currentVideoIndex + 1];
                if (nextVideo) {
                    const playButton = nextVideo.children[0];
                    playButton.click();
                } else {
                    console.log('自动播放 -- 没有更多的视频可以播放');
                }
            }, delay);
        }

        /**
         * 获取当前正在播放的视频在视频列表中的索引。
         * @param {HTMLElement[]} list - 视频元素的列表。
         * @returns {number} - 当前播放视频的索引，如果没有正在播放的视频则返回-1。
         */
        function getCurrentVideoIndex(list) {
            for (let i = 0; i < list.length; i++) {
                let item = list[i];
                if (item.children[0].className === 'isVideo onLive') {
                    return i;
                }
            }
            return -1; // 如果没有找到当前播放的视频，返回-1或其他标识
        }
    }

})();
